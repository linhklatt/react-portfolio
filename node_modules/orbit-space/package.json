{
  "name": "orbit-space",
  "private": false,
  "version": "1.1.0",
  "description": "React 3D background component",
  "type": "module",
  "license": "MIT",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
